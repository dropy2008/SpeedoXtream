<resources>

    <bool name="override_all_contents_subscription_flag">true</bool>
    <!-- Flag for enabling Amazon In App Purchase -->
    <bool name="is_iap_disabled">false</bool>
    <!-- Flag for enabling Launcher integration -->
    <bool name="is_launcher_integration_enabled">true</bool>
    <!-- your partner Id provided during catalog integration process -->
    <string name="launcher_integration_partner_id">your_partner_id</string>
    <!-- Field in the catalog file which matches with the content id  -->
    <string name="launcher_integration_content_id_key">catalog_content_key</string>
    <!-- Your app name to be displayed by launcher -->
    <string name="launcher_integration_app_display_name">Fire App Builder</string>
    <!--Your package name, should be same as in AndroidManifest-->
    <string name="launcher_integration_intent_package_value">com.fireappbuilder.android.calypso</string>
    <!-- Activity to start on receiving broadcasts from Launcher -->
    <string name="launcher_integration_intent_class_value">"com.amazon.android.tv.tenfoot.ui.activities.SplashActivity</string>
    <!-- Default value for MVPD URL. Replace in app's custom.xml with real URL. This string should
    not change here -->
    <string name="mvpd_url">DEFAULT_MVPD_URL</string>
    <!-- App icon -->
    <drawable name="app_logo">@mipmap/ic_launcher</drawable>
    <!-- A grace period used when selecting recent records that have been started but not
    finished. Measured in milliseconds. Default of 10 seconds.-->
    <integer name="recent_grace_period">10000</integer>
</resources>